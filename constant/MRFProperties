FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      MRFProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

MRF1
{
    cellZone    rotatingZone;
    active      yes;

    nonRotatingPatches ();

    origin    (0 0 0.075);
    axis      (0 1 0);
    omega     constant 100;	//ca. 955 U/min
}


