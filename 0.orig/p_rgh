FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
	#includeEtc "caseDicts/setConstraintTypes"
    ofen_walls
    {
        type            fixedFluxPressure;
        value           uniform 1e5;
    }

    keksen
    {
        type            fixedFluxPressure;
        value           uniform 1e5;
    }

    hot_wall
   {
    type            fixedFluxPressure;
    value           uniform 1e5;
   }

glass
{
    type            fixedFluxPressure;
    value           uniform 1e5;
}

trays
{
    type            fixedFluxPressure;
    value           uniform 1e5;
}

fan
{
    type            zeroGradient;

}
}


